{
  "rules": {
    "strict": 0,
    "new-cap": 0,
    "indent": [1, 2],
    "quotes": [1, "single"],
    "semi": [1, "always"],
    "comma-dangle": [1, "always-multiline"],
    "linebreak-style": [2, "unix"],
  },
  "env": {
    "es6": true,
    "browser": true,
    "amd": true,
    "node": true,
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  }
}